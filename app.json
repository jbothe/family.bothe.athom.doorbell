{
  "id": "family.bothe.athom.doorbell",
  "version": "1.0.1",
  "compatibility": ">=1.5.0",
  "sdk": 2,
  "name": {
    "en": "Doorbell"
  },
  "description": {
    "en": "My cheap 433MHz doorbell."
  },
  "category": [
    "security"
  ],
  "permissions": [
    "homey:wireless:433"
  ],
  "author": {
    "name": "John Bothe",
    "email": "jbothe@gmail.com"
  },
  "source": "https://github.com/jbothe/family.bothe.athom.doorbell",
  "drivers": [
    {
      "id": "doorbell",
      "name": {
        "en": "Doorbell"
      },
      "class": "doorbell",
      "capabilities": [],
      "pair": [
        {
          "id": "list_devices",
          "template": "list_devices",
          "navigation": {
            "next": "add_my_devices"
          }
        },
        {
          "id": "add_my_devices",
          "template": "add_devices"
        }
      ]
    }
  ],
  "flow": {
    "triggers": [
      {
        "id": "button_pressed",
        "title": {
          "en": "Doorbell button pressed"
        }
      }
    ]
  },
  "signals": {
    "433": {
      "doorbell": {
        "sof": [
          280,
          9000
        ],
        "eof": [],
        "words": [
          [
            275,
            900
          ],
          [
            880,
            310
          ]
        ],
        "interval": 5,
        "sensitivity": 0.5,
        "repetitions": 20,
        "minimalLength": 24,
        "maximalLength": 24,
        "txOnly": false
      }
    }
  }
}
